{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Express",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server/express/index.js",
      "outFiles": [
        "${workspaceFolder}/server/express/*.js"
      ],
      "cwd": "${workspaceRoot}/server/express",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "args": [
          "${workspaceFolder}/server/nestjs/src/main.ts"
      ],
      "runtimeArgs": [
          "--nolazy",
          "-r",
          "ts-node/register",
          "-r",
          "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}/server/nestjs",
      "console": "integratedTerminal",
      "protocol": "inspector"
    },
    {
      "name": "node-dapp",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "/Users/john/.nvm/versions/node/v14.15.1/bin/node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only"],

      "args": ["index.ts"],
      
      "cwd": "${workspaceRoot}/client/node-dapp",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      // "outFiles": [
      //   "${workspaceRoot}/dist/*.js"
      // ]
    },
    {
      "name": "delegate-auth",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "/Users/john/.nvm/versions/node/v14.15.1/bin/node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only"],

      "args": ["delegate-auth-script.ts"],
      
      "cwd": "${workspaceRoot}/client/delegate-auth",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      // "outFiles": [
      //   "${workspaceRoot}/dist/*.js"
      // ]
    },
  ]
}